{
  "Urls": "http://localhost:4000;https://localhost:4001",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "minimumroute": {
        "WeightClusters": [
          {
            "ClusterId": "minimumcluster",
            "Weight":50
          },
          {
            "ClusterId": "cluster2",
            "Weight":50
          }
        ],
        "Match": {
          "Path": "{**catch-all}"
        }
      },
      "route2": {
        "ClusterId": "cluster2",
        "Match": {
          "Path": "/something/{*any}"
        }
      }
    },
    "Clusters": {
      "minimumcluster": {
        "Destinations": {
          "example.com": {
            "Address": "http://www.example.com/"
          }
        }
      },
      "cluster2": {
        "Destinations": {
          "first_destination": {
            "Address": "http://localhost:9901/"
          }
        },
        "LoadBalancingPolicy": "PowerOfTwoChoices"
      }
    }
  }
}
